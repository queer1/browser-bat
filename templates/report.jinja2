<!DOCTYPE html>
<html>
  <head>
    <title>Browser Bat</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,700" rel="stylesheet" />
    <link rel="stylesheet" href="/static/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/static/css/highslide.css" />
    <link rel="stylesheet" href="/static/css/jquery.dataTables.css" />
    <link rel="stylesheet" href="/static/css/TableTools.css" />
    <link rel="stylesheet" href="/static/css/style.css" />
    <style>
      table { table-layout: fixed; }
      td { word-wrap: break-word; overflow: hidden; }
    </style>
  </head>
  <body>
    <div class="container" id="main">
      <h1>Browser Bat Report</h1>

      <ul class="nav nav-tabs">
        <li class="active"><a href="#visits" data-toggle="tab">Visits</a></li>
        <li><a href="#downloads" data-toggle="tab">Downloads</a></li>
      </ul>

      <div class="tab-content">
        <div class="tab-pane active" id="visits">
          <table id="visits_table">
            <thead>
              <tr>
                <th style="width: 65%">URL</th>
                <th style="width: 15%">Date/time</th>
                <th>Duration</th>
                <th>Browser</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>
        </div>
        <div class="tab-pane" id="downloads">
          <table id="downloads_table">
            <thead>
              <tr>
                <th style="width: 85%">Path</th>
                <th style="width: 15%">Browser</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>
        </div>
      </div>


    </div>
    <script src="/static/js/vendor/jquery-2.0.3.min.js"></script>
    <script src="/static/js/vendor/bootstrap.min.js"></script>
    <script src="/static/js/vendor/jquery.dataTables.min.js"></script>
    <script src="/static/js/vendor/TableTools.min.js"></script>
    <script src="/static/js/vendor/ZeroClipboard.js"></script>
    <script src="/static/js/report.js"></script>
  </body>
</html>
